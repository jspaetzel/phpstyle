{
  "name": "jspaetzel/phpstyle",
  "description": "PHPStyle - Automated, opinionated styling for PHP",
  "license": ["MIT"],
  "prefer-stable": true,
  "require": {
    "php": "^7.1|^8.0",
    "nette/neon": "^3.3"
  },
  "bin": [
    "phpstyle-setup"
  ],
  "autoload": {
    "psr-4": {
      "PHPStyle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PHPStyle\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "3.5",
    "phpunit/phpunit": "^9"
  }
}
